include $(TOP)/config/make.cfg

#MM demo-flamme : demo-flamme-icons

DESTDIR := $(CONTRIBDIR)/Demos/Flamme

%build_prog mmake=demo-flamme \
    progname=Flamme targetdir=$(DESTDIR) \
    files=flamme uselibs="arossupport amiga arosc m"


ICONS := Flamme
ICONFILES := $(foreach f, $(ICONS), $(DESTDIR)/$(f).info)

#MM demo-flamme-icons : demo-flamme-setup
demo-flamme-icons : $(ICONFILES)
	
$(DESTDIR)/%.info : icons/%.info.src icons/%_N.png icons/%_S.png
	%makeicon2
