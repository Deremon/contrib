include $(TOP)/config/make.cfg

#MM demo-tunnel : demo-tunnel-icons demo-tunnel-data

DESTDIR := $(CONTRIBDIR)/Demos/Tunnel

%build_prog mmake=demo-tunnel \
    progname=Tunnel targetdir=$(DESTDIR) \
    files=tunnel uselibs="arossupport amiga arosc m"


DATAFILES	:= pattern.data pattern.pal

#MM demo-tunnel-data
%copy_files_q files=$(DATAFILES) dst=$(DESTDIR) \
    maketarget=demo-tunnel-data

$(addprefix $(DESTDIR)/,$(DATAFILES)) : | $(DESTDIR)

%rule_makedirs dirs=$(DESTDIR)

%common
