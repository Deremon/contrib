            <Group Columns="2" Frame="$group-frame" Title="Active">
                <HLabel>PreParse</HLabel>
                <PopPreParse name="decorator-preparse-active" />
                <HLabel>Scheme</HLabel>
                <PopScheme name="decorator-scheme-active" />
            </Group>

            <Group Columns="2" Frame="$group-frame" Title="Inactive">
