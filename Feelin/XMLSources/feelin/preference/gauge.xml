<?xml version="1.0" ?>
   
<feelin:objects>
   <HGroup>
      <Group Rows="2"  Frame="$group-frame" Title="Aspect" VSpacing="2" SameSize="true">
         <PopFrame name="gauge-frame" />
         <PopScheme name="gauge-scheme" />
         <VLabel>Frame</VLabel>
         <VLabel>Scheme</VLabel>
      </Group>
      
      <!--Bar Title="Back" /-->
      
      <Group Rows="2" Frame="$group-frame" Title="Back" VSpacing="2" SameSize="true">
         <PopImage name="gauge-back" />
         <PopImage name="gauge-back-vertical" />
         <VLabel>Horizontal</VLabel>
         <VLabel>Vertical</VLabel>
      </Group>
   </HGroup>

   <Group Frame="$group-frame" Title="Example">
      <Gauge name="example" Horizontal="true" SetMax="Height" ChainToCycle="false" Max="100" />
   </Group>
</feelin:objects>
