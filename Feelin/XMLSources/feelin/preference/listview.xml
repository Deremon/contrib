<?xml version="1.0" ?>

<feelin:objects>
   <Group>
      <HGroup>
         <Group Frame="$group-frame" Title="Input">
            <Group Rows="2" VSpacing="2">
               <PopFrame name="list-frame" />
               <PopImage name="list-back" />
               <PopScheme name="list-scheme" />
               <VLabel>Frame</VLabel>
               <VLabel>Back</VLabel>
               <VLabel>Scheme</VLabel>
            </Group>
            
            <Bar Title="Title Cells" />
            
            <Group Rows="2" VSpacing="2">
               <PopFrame name="list-title-frame" />
               <PopImage name="list-title-back" />
               <PopScheme name="list-title-scheme" />
               <VLabel>Frame</VLabel>
               <VLabel>Back</VLabel>
               <VLabel>Scheme</VLabel>
            </Group>
         </Group>
         
         <Group Frame="$group-frame" Title="Read">
            <Group Rows="2" VSpacing="2">
               <PopFrame name="list-read-frame" />
               <PopImage name="list-read-back" />
               <PopScheme name="list-read-scheme" />
               <VLabel>Frame</VLabel>
               <VLabel>Back</VLabel>
               <VLabel>Scheme</VLabel>
            </Group>

            <Bar Title="Title Cells" />

            <Group Rows="2" VSpacing="2">
               <PopFrame name="list-read-title-frame" />
               <PopImage name="list-read-title-back" />
               <PopScheme name="list-read-title-scheme" />
               <VLabel>Frame</VLabel>
               <VLabel>Back</VLabel>
               <VLabel>Scheme</VLabel>
            </Group>
         </Group>
      </HGroup>

   
      <HGroup>
         <Group Rows="2" Frame="$group-frame" Title="Cursor" VSpacing="2">
            <PopImage name="list-cursor-active" />
            <PopImage name="list-cursor-selected" />
            <VLabel>Active</VLabel>
            <VLabel>Selected</VLabel>
         </Group>
         
         <Group>   
            <Group Columns="2" Frame="$group-frame" Title="Fonts">
               <HLabel>Normal</HLabel>
               <PopFont name="list-font-prop" />
               <HLabel>Fixed</HLabel>
               <PopFont name="list-font-fixed" />
               <HLabel>Spacing</HLabel>
               <Slider name="list-spacing" Horizontal="true" SetMax="Height" Max="9" />
            </Group>

            <HGroup Frame="$group-frame" Title="Wheel">
               <HLabel>Steps</HLabel>
               <Slider name="list-steps" Horizontal="true" SetMax="Height" Max="10" />
            </HGroup>
         </Group>
      </HGroup>
   </Group>
</feelin:objects>
