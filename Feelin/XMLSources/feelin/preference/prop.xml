<?xml version="1.0" ?>

<feelin:objects>
	<HGroup>
		<Group Frame="$group-frame" Title="msg:container">
			<Group Rows="2" SameWidth="true" VSpacing="2">
				<PopFrame name="prop-frame" />
				<PopScheme name="prop-scheme" />
				<VLabel Text="msg:frame" />
				<VLabel Text="msg:scheme" />
			</Group>
			
			<Bar Title="Back" />
			
			<Group Rows="2" SameWidth="true" VSpacing="2">
				<PopImage name="prop-back" />
				<PopImage name="prop-back-vertical" />
				<VLabel>Horizontal</VLabel>
				<VLabel>Vertical</VLabel>
			</Group>
		</Group>

		<Group Frame="$group-frame" Title="msg:knob">
			<Group Rows="2" SameWidth="true" VSpacing="2">
				<PopFrame name="prop-knob-frame" Couple="true"/>
				<PopScheme name="prop-knob-scheme" />
				<VLabel Text="msg:frame" />
				<VLabel Text="msg:scheme" />
			</Group>
			
			<Bar Title="Back" />
			
			<Group Rows="2" SameWidth="true" VSpacing="2">
				<PopImage name="prop-knob-back" Couple="true" />
				<PopImage name="prop-knob-back-vertical" Couple="true" />
				<VLabel>Horizontal</VLabel>
				<VLabel>Vertical</VLabel>
			</Group>
		</Group>
	</HGroup>
	
	<Balance />

	<HGroup Frame="$group-frame" Title="msg:examples" Weight="25">
		<Group>
			<Prop Horizontal="true" Entries="20" Visible="2" FixHeight="20" />
			<Prop Horizontal="true" Entries="20" Visible="4" FixHeight="20" />
		</Group>

		<Prop Entries="20" Visible="2" FixWidth="20" />
		<Prop Entries="20" Visible="4" FixWidth="20" />
	</HGroup>
</feelin:objects>
  
