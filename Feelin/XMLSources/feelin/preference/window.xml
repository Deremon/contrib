<?xml version="1.0" ?>

<feelin:objects>
   <HGroup>
      <Group Rows="2" Frame="$group-frame" Title="General" VSpacing="2">
         <PopFrame name="window-frame" />
         <PopImage name="window-back" />
         <VLabel>Frame</VLabel>
         <VLabel>Back</VLabel>
      </Group>
    
      <Group Rows="2" Frame="$group-frame" Title="FileDialog" VSpacing="2">
         <PopFrame name="filedialog-frame" />
         <PopImage name="filedialog-back" />
         <VLabel>Frame</VLabel>
         <VLabel>Back</VLabel>
      </Group>

      <Group Rows="2" Frame="$group-frame" Title="FontDialog" VSpacing="2">
         <PopFrame name="fontdialog-frame" />
         <PopImage name="fontdialog-back" />
         <VLabel>Frame</VLabel>
         <VLabel>Back</VLabel>
      </Group>
   </HGroup>

   <Group Rows="2" VSpacing="2">
      <Checkbox name="window-render-complex" />
      <Checkbox name="window-refresh-simple" />
      <VLabel Text="Complex Render" />
      <VLabel Text="Simple Refresh" />
   </Group>
</feelin:objects>
 
