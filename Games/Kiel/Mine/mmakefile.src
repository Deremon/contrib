include $(TOP)/config/make.cfg

FILES := Mine
DESTDIR := $(CONTRIBDIR)/Games/Kiel

#MM- contrib-games : games-kiel-mine

%build_progs mmake=games-kiel-mine \
    files=$(FILES) targetdir=$(DESTDIR) \
    uselibs="arossupport amiga arosc m"


