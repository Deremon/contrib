# Makefile for AiRcOS Internet Relay Chat

include $(TOP)/config/make.cfg

FILES  := locale aircos_main aircos_gui aircos_server_gui aircos_funcs_irc aircos_funcs_dcc

EXEDIR := $(CONTRIBDIR)/Networking/Apps/AiRcOS

USER_CFLAGS := -Wno-uninitialized -Wno-parentheses -I $(TOP)/contrib/Networking/Stacks/AROSTCP/netinclude

#MM aros-tcpip-apps-aircos : aros-tcpip aircos-gadgets \
#MM aros-tcpip-apps-aircos-catalogs

%build_prog mmake=aros-tcpip-apps-aircos \
    progname=AiRcOS targetdir=$(EXEDIR) \
    files=$(FILES) uselibs="mui arossupport amiga arosc"

%common
