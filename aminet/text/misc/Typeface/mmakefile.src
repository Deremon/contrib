# Makefile to make Typeface.

include $(TOP)/config/make.cfg

FILES  := char checkbox error node locale \
          gadget text fontio typeface

EXEDIR := $(CONTRIBDIR)/aminet/
EXE    := $(EXEDIR)/Typeface

USE_AROSLIB  := yes
USE_CLIB     := yes
USE_AMIGALIB := yes
USE_MLIB     := yes
USE_BGUILIB  := yes

USER_CFLAGS := -O2 -Wunused -Wall -DINTUITION_NO_INLINE_STDARG

#MM aminet-text-misc-typeface : includes linklibs setup-typeface-catalogs

%make_prog_setup mmake=aminet-text-misc-typeface

aminet-text-misc-typeface : setup $(EXE)

%make_prog_multi dir=$(EXEDIR) target=$(EXE)

%common
%include_deps
