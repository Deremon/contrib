#   Makefile to make UnZip.

include $(TOP)/config/make.cfg

FILES := unzip crypt envargs explode extract file_io inflate match\
unreduce unshrink zipinfo unix


EXEDIR := $(CONTRIBDIR)/aminet

USER_CFLAGS := -DUNIX -DNO_ZIPINFO -DBSD -DBSD4_4

#MM aminet-util-arc-unzip : includes linklibs

%build_prog mmake=aminet-util-arc-unzip \
    progname=unzip targetdir=$(EXEDIR) \
    files=$(FILES) uselibs="arossupport amiga arosc m"

%common

