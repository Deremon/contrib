USER_CFLAGS := -D_AROS -O2 -Wunused -Wall

#MM bgui-prefs : includes linklibs

%make_prog_setup mmake=bgui-prefs

bgui-prefs : setup $(EXE)

%make_prog_multi dir=$(EXEDIR) target=$(EXE)

%common
%include_deps
