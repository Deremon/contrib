#   Makefile to make lcc.

include $(TOP)/config/make.cfg

FILES := lcc linux

EXEDIR := $(CONTRIBDIR)/aminet/lcc
EXE := $(EXEDIR)/lcc

USE_AROSLIB := yes
USE_CLIB_SHARED := yes
USE_AMIGALIB := yes
USE_MLIB := yes

USER_CFLAGS := -DAROS -DV39 

#MM aminet-development-compiler-lcc-etc

%make_prog_setup mmake=aminet-development-compiler-lcc-etc

aminet-development-compiler-lcc-etc : setup $(EXE)

%make_prog_multi dir=$(EXEDIR) target=$(EXE)

%common
%include_deps
