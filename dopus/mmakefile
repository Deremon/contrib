include make.cfg

SFILES := Package-Startup

#MM contrib-dopus : dopus-library DirOpus ConfigOpus
contrib-dopus : $(SFILES:%=$(DOPUSDIR)/S/%)

$(DOPUSDIR)/S/% : S/%
	@$(MKDIR) $(dir $@)
	@$(CP) $< $@

#MM
contrib-dopus :
	$(ECHO) "Extras:DOpus" >$(AROS_PACKAGES)/DOpus
