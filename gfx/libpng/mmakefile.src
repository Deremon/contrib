include $(TOP)/config/make.cfg

##MM- linklibs : linklibs-png
##MM linklibs-pnglib : linklibs-png
##MM linklibs-png : includes
##MM includes-copy

INCLUDE_FILES	:= png.h pngconf.h

%copy_includes path=. dir=

FILES := \
    png \
    pngerror \
    pngget \
    pngmem \
    pngpread \
    pngread \
    pngrio \
    pngrtran \
    pngrutil \
    pngset \
    pngtrans \
    pngwio \
    pngwrite \
    pngwtran \
    pngwutil

USER_INCLUDES	:= -Iinclude
USER_CFLAGS	:= -DAROS -DNO_ERRNO_H

%build_linklib mmake=linklibs-png-disabled libname=png files=$(FILES)

%common
