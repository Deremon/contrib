include $(TOP)/config/make.cfg

#MM linklibs-mesaglu : includes includes-copy

LIBMESAGLU =  glu \
            mipmap \
            nurbs \
            nurbscrv \
            nurbssrf \
            nurbsutl \
            project \
            quadric \
            tess \
