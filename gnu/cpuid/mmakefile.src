include $(SRCDIR)/$(CURDIR)/../make.cfg
-include $(GENDIR)/config/compiler.cfg

EXEDIR := $(GNUDIR)/cpuid/bin
FILES  := cpuid

NOWARN_FLAGS := $(NOWARN_UNUSED_BUT_SET_VARIABLE)
USER_CFLAGS += $(NOWARN_FLAGS)

#MM- contrib-gnu-cpuid : cpuid-$(AROS_TARGET_CPU)
