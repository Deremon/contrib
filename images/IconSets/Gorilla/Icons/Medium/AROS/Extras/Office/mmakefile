include $(TOP)/config/make.cfg

#MM- iconset-Gorilla-contrib-icons-extras : iconset-Gorilla-contrib-icons-extras-office \
#MM  iconset-Gorilla-contrib-icons-extras-office-muibase

ICONS := MUIbase

#MM- iconset-Gorilla-contrib-icons-extras-clean : iconset-Gorilla-contrib-icons-extras-office-clean

BD_OBJS := $(addprefix  $(CONTRIBDIR)/Office/, $(addsuffix .info,$(ICONS)))

#MM
iconset-Gorilla-contrib-icons-extras-office : $(BD_OBJS)

ifeq (,)

$(CONTRIBDIR)/Office/%.info : $(SRCDIR)/$(CURDIR)/%.info.src $(SRCDIR)/$(CURDIR)/%.png
	@$(ECHO) "Creating   $(subst $(TARGETDIR)/,,$@)..."
	@$(ILBMTOICON) $+ $@

else

$(CONTRIBDIR)/Office/%.info : $(SRCDIR)/$(CURDIR)/.info.src $(SRCDIR)/$(CURDIR)/
	@$(ECHO) "Creating   $(subst $(TARGETDIR)/,,$@)..."
	@$(ILBMTOICON) $+ $@

endif

$(BD_OBJS) : | $(CONTRIBDIR)/Office
GLOB_MKDIRS += $(CONTRIBDIR)/Office

iconset-Gorilla-contrib-icons-extras-office-clean : FILES := $(BD_OBJS)
#MM
iconset-Gorilla-contrib-icons-extras-office-clean ::
	@$(RM) $(FILES)

.PHONY: iconset-Gorilla-contrib-icons-extras-office iconset-Gorilla-contrib-icons-extras-office-clean


# Delete generated makefiles
#MM
clean ::
	@$(RM) $(TOP)/$(CURDIR)/mmakefile $(TOP)/$(CURDIR)/mmakefile.bak

include $(SRCDIR)/config/make.tail

BDID := $(BDTARGETID)
