TRGDIR = ./
FILES = $(TRGDIR)/tcc

EXEDIR := $(CONTRIBDIR)/tcc
EXE    := $(EXEDIR)/tcc

USE_CLIB_SHARED := yes
USE_AMIGALIB := yes
USE_MLIB := yes

USER_CFLAGS := -O2 -g -Wall -mcpu=i686  -DCONFIG_TCC_STATIC
#MM- contrib-development-compilers-tcc : tcc
#MM- contrib-development-compilers-tcc : contrib-development-compilers-tcc-libtcc1
#MM
tcc : setup $(EXE) cleanup

$(OBJDIR)/%.o: %.S
	@$(ECHO) "Compiling $(CURDIR)/$(patsubst $(CURDIR)//,,$<)..."
	@$(IF) $(CC) $(USER_CFLAGS) -c $< -o $@ > $(GENDIR)/cerrors 2>&1 ; then \
	    $(IF) $(TEST) -s $(GENDIR)/cerrors ; then \
		$(ECHO) "$(CURDIR)/$<: $(CC) $(USER_CFLAGS) -c $< -o $@" >> $(GENDIR)/errors ; \
		$(CAT) $(GENDIR)/cerrors >> $(GENDIR)/errors ; \
	    else \
		$(NOP) ; \
	    fi ; \
	else \
	    $(ECHO) "Compile failed: $(CC) $(USER_CFLAGS) -c $< -o $@" 1>&2 ; \
	    tee < $(GENDIR)/cerrors -a $(GENDIR)/errors 1>&2 ; \
	    exit 1 ; \
	fi
cleanup :: 
	rm -rf tmp.c
.PHONY : tcc tcc-clean tcc-quick
#MM
tcc-clean : clean
#MM
tcc-quick : tcc

